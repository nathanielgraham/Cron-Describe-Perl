[
    {
        "description": "Validating last day of month Quartz pattern",
        "expression": "0 0 0 L * ? *",
        "expected_valid": true,
        "match_tests": [
            {
                "timestamp": 1767139200,
                "expected_match": true
            }
        ]
    },
    {
        "description": "Validating third Sunday Quartz pattern",
        "expression": "0 0 0 ? * 1#3 *",
        "expected_valid": true,
        "utc_offset": 330,
        "match_tests": [
            {
                "timestamp": 1763231400,
                "expected_match": true
            }
        ]
    },
    {
        "description": "Validating nearest weekday to 15th Quartz pattern",
        "expression": "0 0 0 15W * ? *",
        "expected_valid": true,
        "match_tests": [
            {
                "timestamp": 1765756800,
                "expected_match": true
            }
        ]
    },
    {
        "description": "Validating third-to-last day Quartz pattern",
        "expression": "0 0 0 L-3 * ? *",
        "expected_valid": true
    },
    {
        "description": "Valid cron with all wildcards",
        "expression": "0 0 0 * * ? *",
        "expected_valid": true,
        "match_tests": [
            {
                "timestamp": 1767139200,
                "expected_match": true
            },
            {
                "timestamp": 1767142800,
                "expected_match": false
            }
        ]
    },
    {
        "description": "Valid cron with specific time",
        "expression": "0 15 10 * * ? *",
        "expected_valid": true
    },
    {
        "description": "Invalid sixth Sunday Quartz pattern",
        "expression": "0 0 0 ? * 1#6 *",
        "expected_valid": false,
        "error_message": "Nth value 6 out of range (1-5)"
    },
    {
        "description": "Invalid minute value",
        "expression": "0 60 0 * * ? *",
        "expected_valid": false,
        "error_message": "Invalid value '60' for minute, expected 0-59"
    },
    {
        "description": "Invalid day for February",
        "expression": "0 0 0 31 2 ? *",
        "expected_valid": false,
        "error_message": "Day 31 is invalid for February (month 2)"
    },
    {
        "description": "Invalid combination of day-of-month and day-of-week",
        "expression": "0 0 0 LW * 1 *",
        "expected_valid": false,
        "error_message": "Cannot specify both day-of-month and day-of-week; one must be '?'"
    },
    {
        "description": "Invalid day-of-week pattern",
        "expression": "0 0 0 * * ?W *",
        "expected_valid": false,
        "error_message": "Invalid use of 'W' in day-of-week field"
    },
    {
        "description": "Invalid out-of-range nearest weekday",
        "expression": "0 0 0 32W * ? *",
        "expected_valid": false,
        "error_message": "Day 32 is out of range for nearest weekday (1-31)"
    },
    {
        "description": "Invalid day-of-week number",
        "expression": "0 0 0 * * 8#3 *",
        "expected_valid": false,
        "error_message": "Day 8 out of range for dow"
    },
    {
        "description": "Invalid day-of-week pattern",
        "expression": "0 0 0 * * W#3 *",
        "expected_valid": false,
        "error_message": "Invalid use of 'W' in day-of-week field"
    },
    {
        "description": "Invalid combination of day-of-month and day-of-week",
        "expression": "0 0 0 15 * 1 *",
        "expected_valid": false,
        "error_message": "Cannot specify both day-of-month and day-of-week; one must be '?'"
    },
    {
        "description": "Invalid year value",
        "expression": "0 0 0 * * ? 1969",
        "expected_valid": false,
        "error_message": "Invalid value '1969' for year, expected 1970-2099"
    },
    {
        "description": "Valid day-of-month list",
        "expression": "0 0 0 1,15,31 * ? *",
        "expected_valid": true
    },
    {
        "description": "Invalid day-of-month value",
        "expression": "0 0 0 32 * ? *",
        "expected_valid": false,
        "error_message": "Invalid value '32' for dom, expected 1-31"
    },
    {
        "description": "Invalid step value",
        "expression": "0 0 0 0/0 * ? *",
        "expected_valid": false,
        "error_message": "Step value 0 is invalid for dom"
    },
    {
        "description": "Invalid month value",
        "expression": "0 0 0 * 13 ? *",
        "expected_valid": false,
        "error_message": "Invalid value '13' for month, expected 1-12"
    },
    {
        "description": "Empty cron expression",
        "expression": "",
        "expected_valid": false,
        "error_message": "Empty cron expression is invalid"
    }
]
